	'bundles/uamdatatables/js/jquery.dataTables.min.js'
	'bundles/uamdatatables/js/jquery.dataTables_bootstrap.js'
	'bundles/dzangocartcore/js/scripts/order/orders.js'
    'bundles/uambootstrap/vendor/typeahead.js/dist/typeahead.bundle.min.js'
    'bundles/uambootstrap/vendor/typeahead.js/dist/typeahead.jquery.min.js'
%}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>
dzangocart.orders = {
