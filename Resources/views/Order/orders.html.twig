{% trans_default_domain "order" %}

<table class="table table-condensed">
    <thead>
        <tr>
            <th class="selectall"></th>
            <th class="datetime">{% trans %}order.orders.date{% endtrans %}</th>
            <th class="id">{% trans %}order.orders.id{% endtrans %}</th>
            <th class="store">{% trans %}order.orders.store_name{% endtrans %}</th>
            <th class="customer">{% trans %}order.orders.customer{% endtrans %}</th>
            <th class="status">{% trans %}order.orders.status{% endtrans %}</th>
            <th class="currency">{% trans %}order.orders.currency{% endtrans %}</th>
            <th class="gross">{% trans %}order.orders.gross{% endtrans %}</th>
            <th class="tax">{% trans %}order.orders.tax{% endtrans %}</th>
            <th class="net">{% trans %}order.orders.net{% endtrans %}</th>
            <th class="actions">{% trans %}order.orders.actions.title{% endtrans %}</th>
        </tr>
        <tr class="filters">
            <th></th>
            <th></th>
            <th class="filters_keyup">{{ form_widget(form.id) }}</th>
            <th class="filters_keyup">{{ form_widget(form.store_name) }}</th>
            <th class="customer_filters">{{ form_widget(form.customer) }}{{ form_widget(form.customer_id) }}</th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
       </tr>
    </thead>
    <tbody>
    </tbody>
</table>
