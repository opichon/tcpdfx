{% javascripts
	'bundles/dzangocartcore/vendor/datatables/media/js/jquery.dataTables.min.js'
	'bundles/dzangocartcore/vendor/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js'
	'bundles/dzangocartcore/vendor/typeahead.js/dist/typeahead.bundle.min.js'
	'bundles/dzangocartcore/vendor/moment/min/moment-with-langs.min.js'
	'bundles/dzangocartcore/vendor/bootstrap-daterangepicker/daterangepicker.min.js'
	'bundles/dzangocartcore/js/ajax.js'
	'bundles/dzangocartcore/js/payment/payments.js'
%}
	<script src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>
dzangocart.payments = {
    dataTables: {
        ajax: {
            url: "{{ path('payments_list') }}"
        }
    },
    daterangepicker: {
        ranges: {% include "DzangocartCoreBundle::date_ranges.html.twig" %}
    },
    typeahead: {
        remote: {
            url:  "{{ path('customer_search', { search: '__query__' }) }}"
        }
    }
};
</script>
