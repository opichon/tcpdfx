{% trans_default_domain "payment" %}

<table class="table table-condensed">
    <thead>
        <tr>
            <th class="selectall"></th>
            <th class="datetime">{% trans %}payment.payments.date{% endtrans %}</th>
            <th class="store">{% trans %}payment.payments.store{% endtrans %}</th>
            <th class="id">{% trans %}payment.payments.order_id{% endtrans %}</th>
            <th class="classkey">{% trans %}payment.payments.class_key{% endtrans %}</th>
            <th class="amount">{% trans %}payment.payments.transaction.amount{% endtrans %}</th>
            <th class="status">{% trans %}payment.payments.status{% endtrans %}</th>
            <th class="actions">{% trans %}payment.payments.actions.title{% endtrans %}</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
