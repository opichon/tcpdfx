{% spaceless %}
{
    "sEcho": "{{ sEcho }}",
    "iTotalRecords": {{ iTotalRecords }},
    "iTotalDisplayRecords": {{ iTotalDisplayRecords }},
    "aaData" : [
        {% for user in users %}
            {
                "DT_RowId": "{{ '_' ~ user.id }}",
                "DT_RowClass": "user{{ user.enabled ? '' : ' disabled' }}{{ user.admin ? ' admin' :'' }}{{ user.owner ? ' owner' : '' }}",
                "id": "{{ user.id }}",
                "0": "",
                "1": "{{ user.profile.surname }}",
                "2": "{{ user.profile.givenNames }}",
                "3": "{%if(user.profile.gender == '1')%} m {% elseif(user.profile.gender == '0') %} f {% else %} {% endif %}",
                "4": "{{ user.username }}",
                "5": "{{ user.realm }}",
                "6": "{{ user.code }}",
                "7": "{{ user.createdAt|datetime('dd.MM.yy', 'HH:mm', time_zone, app.request.locale) }}",
                "8": "{{ user.lastLogin|datetime('dd.MM.yy', 'HH:mm', time_zone, app.request.locale) }}",
                "9": ""
            }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}
{% endspaceless %}
