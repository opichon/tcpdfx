{
  "name": "uam/user-admin-bundle",
  "dependencies": {
    "bootstrap": "~3.1",
    "bootstrap-daterangepicker": "~1.3",
    "datatables": "~1.10",
    "datatables-plugins": "*",
    "font-awesome": "~4.1",
    "jquery": "~1.11",
    "jquery-treetable": "~3.1.0",
    "typeahead.js": "~0.10"
  },
  "overrides": {
