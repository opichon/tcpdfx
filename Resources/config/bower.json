{
	"name": "dzangocart/core-bundle",
	"dependencies": {
		"autotabs": "~2.1",
		"bootbox": "~4.3.0",
		"bootstrap-daterangepicker": "~1.3.10",
		"datatables": "~1.10",
		"datatables-plugins": "*",
		"jquery": "~1.11",
		"jquery.cookie": "~1.4",
		"jquery.easing": "~1.3",
		"jquery-form": "~3.46",
		"jquery-treetable": "~3.1.0",
		"moment": "^2.8.1",
		"mustache": "~0.8.2",
		"typeahead.js": "~0.10"
	},
	"overrides": {
		"bootstrap": {
			"ignore": true
		},
		"bootstrap-daterangepicker": {
			"main": [
				"daterangepicker.js",
				"daterangepicker-bs3.css"
			]
		},
		"datatables-plugins": {
			"main": [
				"integration/bootstrap/3/dataTables.bootstrap.css",
				"integration/bootstrap/3/dataTables.bootstrap.js",
				"integration/bootstrap/images/sort_asc.png",
				"integration/bootstrap/images/sort_asc_disabled.png",
				"integration/bootstrap/images/sort_both.png",
				"integration/bootstrap/images/sort_desc.png",
				"integration/bootstrap/images/sort_desc_disabled.png"
			]
		},
		"jquery": {
			"ignore": true
		},
		"jquery-treetable": {
			"main": [
				"javascripts/src/jquery.treetable.js",
				"stylesheets/jquery.treetable.css",
				"stylesheets/jquery.treetable.theme.default.css"
			]
		},
		"moment": {
			"main": [
				"locale/fr.js",
				"min/moment.min.js"
			]
		},
		"typeahead.js": {
			"main": [
				"dist/typeahead.bundle.js",
				"dist/typeahead.bundle.min,js"
			]
		}
	},
	"resolutions": {
		"jquery": "~1.11"
	}
}
